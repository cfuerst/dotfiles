#ubuntu bug with certain keybindings
#https://bugs.launchpad.net/ubuntu/+source/zsh/+bug/1048212
DEBIAN_PREVENT_KEYBOARD_CHANGES=yes

#encoding
LC_ALL=en_US.utf8

#available enviroments
ENV_SYMBOL_HOME="❤"
ENV_SYMBOL_DEV="✪"
ENV_SYMBOL_STAGE="✔"
ENV_SYMBOL_PROD="☢"
#set enviroment
ENV_SYMBOL=$ENV_SYMBOL_HOME
